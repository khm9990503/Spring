<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user3">

	<select id="selectUser3" resultType="kr.co.ch05.vo.User3VO">
		select * from `user3` where `uid`=#{uid};
	</select>
	<select id="selectUser3s" resultType="kr.co.ch05.vo.User3VO">
		SELECT * FROM `user3`;
	</select>
	<insert id="inserUser3">
		insert into `user3` values (#{uid}, #{name}, #{hp}, #{age})
	</insert>	
	<update id="updateUser3">
		update 
	</update>
	<delete id="deleteUser3"></delete>

</mapper>